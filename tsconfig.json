{
  "compilerOptions": {
    "target":"ES2022",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "lib": ["ES2022"],
    "module": "ES2022",
    "moduleResolution": "bundler",
    "outDir": "./lib",
    "strict": true,
    "allowSyntheticDefaultImports": true
  },
  "include":[
    "src/**/*.ts"
  ]
}
